{
  "name": "react-ts-linting",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x} --cache --max-warnings=0",
    "lint:fix": "eslint src/**/*.ts{,x} --fix",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "move-static": "copy -a ./node_modules/@pdftron/webviewer/public/. ./dist/public/webviewer"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@formatjs/intl-pluralrules": "^5.1.10",
    "@mui/material": "^5.11.13",
    "@pdftron/webviewer": "^8.12.0",
    "@reduxjs/toolkit": "^1.9.3",
    "dotenv": "^16.0.3",
    "pspdfkit": "^2023.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.0.0-rc.5",
    "@storybook/addon-interactions": "7.0.0-rc.5",
    "@storybook/addon-links": "7.0.0-rc.5",
    "@storybook/blocks": "7.0.0-rc.5",
    "@storybook/react": "7.0.0-rc.5",
    "@storybook/react-vite": "7.0.0-rc.5",
    "@storybook/testing-library": "0.0.14-next.1",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "prop-types": "15.8.1",
    "storybook": "7.0.0-rc.5",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  }
}
